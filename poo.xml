    xsi:schemaLocation="http://tierno.es/poo poo.xsd">
    <final>
        <definition>Clase final que no admite herencia</definition>
        <declaration languaje="java">public final class [name]</declaration>
        <declaration languaje="php">final class [name] { }</declaration>
    </final>
    <abstract>
        <definition>Clase de la que no pueden instanciar </definition>
        <declaration lang="java">public abstract class [name]...</declaration>
        <declaration lang="php">abstract class []...</declaration>
    </abstract>
</classes>

